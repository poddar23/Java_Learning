<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ordered & Unordered Lists</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
</head>
<body>

    <div class="container mt-5">
        <h2 class="text-center mb-4"> Unordered Lists</h2>

        <!-- Unordered List -->
        <div class="mb-4">
           
            <ul class="list-group">
                <li class="list-group-item">Apple</li>
                <li class="list-group-item">Banana</li>
                <li class="list-group-item">Mango</li>
                <li class="list-group-item">Grapes</li>
                <li class="list-group-item">Pineapple</li>
            </ul>
        </div>

        <!-- Ordered List -->
        <h2 class="text-center mb-4">Ordered Lists </h2>
        
        <div>
            
            <ol class="list-group list-group-numbered">
                <li class="list-group-item">Wake up at 6:00 AM</li>
                <li class="list-group-item">Go to the gym</li>
                <li class="list-group-item">Have breakfast</li>
                <li class="list-group-item">Start working on projects</li>
                <li class="list-group-item">Go for an evening walk</li>
            </ol>
        </div>

    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>