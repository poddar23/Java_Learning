		a.add("Ronak");
		a.add(23);
		a.add('a');
		a.add(10);
		a.add(2,"Ronak");
	}
		ArrayList <Integer> a = new ArrayList<Integer>();
		a.add(44);
		a.add(22);
		a.add(33);
		a.add(420);
//		a.remove(1);
//		a.set(2,44);
//		for (Integer i : a) {
//			System.out.println(i);
//		}
//	}


		b.addAll(a);
		Iterator<Integer> o=b.iterator();
		while(o.hasNext()) {
			System.out.println(o.next());
		}
		
		
}
}
