package assignment;

class MaxOfThree
{
	public static int findMax(int a, int b, int c) {
		
		int max = a;
		
		if (b > max) {
			max = b;
		}
		
		if (c > max) {
			max = c;
		}
		
		return max;
	}
}
public class Lab_5a {
	
	public static void main(String[] args) {
		
		int num1 = 25;
		int num2 = 42;
		int num3 = 55;	
		
		int maximum = findMax(num1, num2, num3);
		
		System.out.println("The maximum of" + num1 +","+ num2 +", and" + num3 +"is:" + maximum);
	}
}
