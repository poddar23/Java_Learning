package assignment;

class MaxOfThree
{
	public static int findMax(int a, int b, int c) {
		
		int max = a;
		
		if (b > max) {
			max = b;
		}
		
		if (c > max) {
			max = c;
		}
	return max;
}
	}
public class Lab_5a {
	
	public static void main(String[] args) {
		
		MaxOfThree m=new MaxOfThree();
		m.findMax(21, 20, 30);
		System.out.println(m);
		
		//System.out.println("The maximum of" + num1 +","+ num2 +", and" + num3 +"is:" + maximum);
	
}
}




